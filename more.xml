        <dep key="Native"/>
        <dep key="path://mt.more/Native/Hash.Extras.js"/>
      </module>
      <module key="path://mt.more/Native/String.QueryString.js">
        <description>String shortcuts for working with query strings.</description>
        <dep key="more.Core"/>
        <dep key="Native"/>
