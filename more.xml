        <dep key="path://mt.more/Class/Class.Refactor.js"/>
        <dep key="path://mt.core/Fx/Fx.Tween.js"/>
        <dep key="path://mt.more/Interface/Mask.js"/>
      </module>
    </package>
  </package>
</js>
