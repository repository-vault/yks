        <dep key="path://mt.more/Fx/Fx.Elements.js"/>
        <dep key="path://mt.core/Element/Element.Dimensions.js"/>
        <dep key="path://mt.more/Element/Element.Measure.js"/>
      </module>
    </package>
    <package name="more.Drag">
