      <module key="path://mt.more/Utilities/Group.js">
        <description>Class for monitoring collections of events.</description>
        <dep key="more.Core"/>
        <dep key="Class"/>
      </module>
      <module key="path://mt.more/Utilities/Hash.Cookie.js">
