        <dep key="more.Core"/>
        <dep key="Element"/>
      </module>
      <module key="path://mt.more/Element/Elements.From.js">
        <description>Adds a static method to Element to parse html.</description>
        <dep key="more.Core"/>
        <dep key="Element"/>
      </module>
