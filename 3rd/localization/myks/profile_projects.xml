<?xml version="1.0"?>
<!DOCTYPE myks PUBLIC "-//YKS//MYKS">
<myks>
  <table name="ks_users_profile_locale_projects" extend="ks_users_profile">
    <fields>
      <field type="user_id" key="primary"/>
      <field type="project_id" key="primary"/>
    </fields>
  </table>
  <table name="ks_users_profile_locale_languages" extend="ks_users_profile">
    <fields>
      <field type="user_id" key="primary"/>
      <field type="lang_key" key="primary" delete="cascade"/>
    </fields>
  </table>
  <table name="ks_users_profile_locale_domains" extend="ks_users_profile">
    <fields>
      <field type="user_id" key="primary"/>
      <field type="locale_domain_id" key="primary" delete="cascade"/>
    </fields>
  </table>
</myks>
